{
  "crons": [
    {
      "path": "/api/cron/generate-week",
      "schedule": "0 5 * * *"
    }
  ],
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "pnpm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        }
      ]
    }
  ]
}
